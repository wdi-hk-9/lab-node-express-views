<h3>Add Quote!</h3>
<fieldset>
  <form method="POST" action="/quotes" class="form form-horizontal">
    <div class="form-group col-md-10">
      <input name="quote[text]" class="form-control" placeholder="This is a quote..."/>
    </div>
    <div class="form-group col-md-10">
      <input name="qupte[author]"  class="form-control" placeholder="Author"/>
    </div>
      <div class="form-group col-md-4">
        <input class="btn btn-default col-md-12" type="submit" value="Save Quote">
      </div>
  </form>
</fieldset>
